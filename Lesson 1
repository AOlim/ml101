def setup():
    size(1000, 1000)
    noFill()
    frameRate(4)

def draw():
    background(255, 255, 255)
    for x in range(50, width - 50, 50):
        for y in range(50, height - 50, 50):                         
            fill(random(0,255), random(0,255),random(0,255))
            rect(x, y + random(0, 100), random(0, 100), random(0, 100))
